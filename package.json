{
  "name": "contact-stress",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cubic-spline": "^1.0.4",
    "jquery": "^3.2.1",
    "ndarray": "^1.0.18",
    "ndarray-linear-interpolate": "^1.0.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "papaparse": "^4.3.6",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-form": "^2.14.4",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-slick": "^0.16.0",
    "redux": "^3.7.2",
    "slick-carousel": "^1.8.1",
    "split-array": "^1.0.1",
    "vis": "^4.21.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar ./src/style/sass -o ./src/style/css",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/style/sass -o ./src/style/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
